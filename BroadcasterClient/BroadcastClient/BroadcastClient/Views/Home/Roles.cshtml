@model IEnumerable<RoleObject>

@{
    ViewData["Title"] = "Roles";
}

<h1>Roles</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NormalizedName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConcurrencyStamp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NormalizedName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConcurrencyStamp)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { roleId=item.Id }) |
                    @Html.ActionLink("Details", "Details", new { roleId=item.Id }) |
                    @Html.ActionLink("DeleteRole", "DeleteRole", new { roleId=item.Id})
                    @Html.ActionLink("EditUsersInRole", "EditUsersInRole", new { roleId=item.Id})
            </td>
        </tr>
}
    </tbody>
</table>
